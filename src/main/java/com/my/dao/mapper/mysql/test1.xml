<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.my.dao.mysql.ITest1Dao">
    <!-- 查询全部用户 -->
    <select id="getTestList" parameterType="java.lang.Integer" resultType="com.my.pojo.Test1Model">
       SELECT
	      id,
	      city,
	      country,
	      remark
        FROM
          test.test1
        limit #{count}
    </select>
</mapper>